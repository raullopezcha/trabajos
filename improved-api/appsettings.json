{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning",
            "System.Net.Http.HttpClient": "Warning"
        }
    },
    "Jwt": {
        "Issuer": "MyApiGateway",
        "Audience": "MyApiClients"
    },
    "ServiceLayer": {
        "BaseUrl": "https://B1SERVER:50000/b1s/v1/"
    },
    "Redis": {
        "Configuration": ""
    },
    "RateLimiting": {
        "PermitLimit": 100,
        "Window": 60,
        "QueueLimit": 10
    },
    "Cors": {
        "AllowedOrigins": []
    },
    "AllowedHosts": "*"
}
